{
  "name": "babel-plugin-list-imports",
  "version": "0.1.0",
  "description": "Lists the files imported by a ESModule file.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci",
    "test:update": "node --experimental-vm-modules node_modules/jest/bin/jest.js -u",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:actions": "npm run test:ci && npm run lint",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "commonignore": "commonignore -c common-ignore/config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChocolateLoverRaj/babel-plugin-list-imports.git"
  },
  "keywords": [
    "babel",
    "plugin",
    "babel-plugin",
    "esm",
    "es",
    "esmodule",
    "imports",
    "list",
    "get"
  ],
  "author": "Rajas Paranjpe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChocolateLoverRaj/babel-plugin-list-imports/issues"
  },
  "homepage": "https://github.com/ChocolateLoverRaj/babel-plugin-list-imports#readme",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "eventemitter3": "^4.0.7"
  },
  "devDependencies": {
    "common-ignore": "^1.1.1",
    "jest": "^26.5.2",
    "pre-commit": "^1.2.2",
    "standard": "^14.3.4"
  },
  "pre-commit": "commonignore"
}